{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue","vite.config.ts,vitest.config.ts","**.d.ts"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "noImplicitAny": false,
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "types": ["vite-plugin-svg-icons"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
